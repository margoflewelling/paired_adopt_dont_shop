<content>
  <h1>My Favorite Pets</h1>
  <% @favorites.each do |pet_id| %>
  <% pet = Pet.find(pet_id) %>
    <section class = "pet-<%="#{pet.id}"%>">
      <div>
        <h2><%= link_to "#{pet.name}", "pets/#{pet.id}" %></h2>
        <img src="<%=pet.image%>">
      </div>
    </section>
  <% end %>
</content>
